{
    "name": "@hydric/gateway",
    "version": "0.0.1",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./types": {
            "types": "./dist/types.d.ts",
            "import": "./dist/types.js",
            "require": "./dist/types.cjs"
        }
    },
    "engines": {
        "node": ">=18"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit",
        "generate-types": "openapi-typescript ../../specs/openapi.json -o src/generated/api-types.ts",
        "test": "vitest run"
    },
    "devDependencies": {
        "@vitest/coverage-v8": "^1.6.1",
        "openapi-typescript": "^6.7.4",
        "tsup": "^8.0.2",
        "tsx": "^4.21.0",
        "vitest": "^1.6.1"
    }
}