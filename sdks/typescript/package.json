{
    "name": "@hydric/gateway",
    "version": "1.0.2",
    "description": "The official JavaScript & TypeScript SDK for the hydric Gateway.",
    "license": "Apache-2.0",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "keywords": [
        "hydric",
        "defi",
        "normalized-data",
        "cross-chain",
        "multi-chain",
        "multichain",
        "liquidity-pool",
        "token",
        "crypto",
        "ethereum",
        "sdk",
        "api-client",
        "viem",
        "ethers",
        "token-basket"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hydric-org/gateway-sdk.git",
        "directory": "sdks/typescript"
    },
    "homepage": "https://docs.hydric.org",
    "bugs": {
        "url": "https://github.com/hydric-org/gateway-sdk/issues"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit",
        "generate-types": "openapi-typescript ../../specs/openapi.json -o src/generated/api-types.ts",
        "test": "vitest run",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@vitest/coverage-v8": "^1.6.1",
        "openapi-typescript": "^6.7.4",
        "tsup": "^8.0.2",
        "tsx": "^4.21.0",
        "vitest": "^1.6.1",
        "@types/node": "^20.0.0"
    },
    "sideEffects": false
}